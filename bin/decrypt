#!/usr/bin/env node

var RSAKey   = require("react-native-rsa");
var key_json = require("../private_key.json");

var key = new RSAKey();
key.setPrivateEx(key_json.n, key_json.e, key_json.d, key_json.p, key_json.q, 
  key_json.dmp1, key_json.dmq1, key_json.coeff);

var encrypted = process.argv[2];
console.log("encrypted: ", encrypted);
 
var decrypted = key.decrypt(encrypted);
console.log('decrypted: ', decrypted);


